{
  "name": "ispy",
  "displayName": "Ispy",
  "version": "0.0.1",
  "description": "A basic Plasmo extension.",
  "author": "maggie",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.11.0",
    "plasmo": "0.67.4",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@plasmohq/prettier-plugin-sort-imports": "3.6.3",
    "@types/chrome": "0.0.210",
    "@types/node": "18.11.18",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "prettier": "2.8.3",
    "typescript": "4.9.4"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "options.html"
      ],
      "matches": [
        "<all_urls>"
      ],
      "extension_ids": [
        "$CRX_ID"
      ]
    }
  ],
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ],
    "key": "$CRX_KEY",
    "permissions": [
      "storage",
      "tabs"
    ]
  }
}
